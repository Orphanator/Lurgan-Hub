Humanoid:AddLabel("Note: Some of these scripts are detectable!")

Humanoid:AddSlider({
	Name = "Speed",
	Min = 0,
	Max = 1000,
	Default = 16,
	Color = Color3.fromRGB(255,0,0),
	Increment = 1,
	ValueName = "Walkspeed",
	Callback = function(e)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = e
	end
	})

	Humanoid:AddSlider({
		Name = "Jump",
		Min = 0,
		Max = 1000,
		Default = 50,
		Color = Color3.fromRGB(255,0,0),
		Increment = 1,
		ValueName = "JumpHeight",
		Callback = function(e)
			game.Players.LocalPlayer.Character.Humanoid.JumpPower = e
		end
		})

	Humanoid:AddSlider({
		Name = "Gravity",
		Min = 0,
		Max = 1000,
		Default = 150,
		Color = Color3.fromRGB(255,0,0),
		Increment = 1,
		ValueName = "Gravity",
		Callback = function(e)
			workspace.Gravity = e
		end
		})


		local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

getgenv().Config = Config or {
    Noclip = false
}

Humanoid:AddBind({
    Name = "Noclip",
    Default = Enum.KeyCode.X,
    Hold = false,
    Callback = function(state)
        Config.Noclip = not Config.Noclip 
        print(Config.Noclip)
        NoclipNotify()
    end
})

local old 
old = hookmetamethod(game, "__newindex", newcclosure(function(self, Meme, Value)
    if self == "HumanoidRootPart" or self == "Torso" and not checkcaller() then 
        if Meme == "CFrame" and self:IsDescendantOf(LocalPlayer.Character) then 
            return true
        end
    end

    return old(self, Meme, Value)
end))

Humanoid:AddLabel("Note: Press PlatForm Fly once and press R to toggle.")

Humanoid:AddButton({
	Name = "R6 Platform Fly",
	Callback = function()
        wait(0.001);
script.Archivable = true;
local scrSource = script:Clone();
scrSource.Parent = nil;
local player = game:FindService'Players'.LocalPlayer;
local mouse = player:GetMouse();
local playerTarget = player.Character;
local partTarget = playerTarget.Torso;
local allParts = {};
local curTransparency = 0;
local addValue = 0;
local rotValueX = 0;
local rotValueY = 0;
local rotValueZ = 0;
local rotAddX = 0;
local rotAddY = 0;
local rotAddZ = 0;
local canCollide = true;
local lastSpaceTick = 0;
local Keys = {
    ["LCtrl"] = 50;
    ["LShift"] = 48;
    ["R"] = "r";
    ["Space"] = " "
};
local ascending = false;
local descending = false;
local brickColor = "Institutional white";
for x = 1,5 do
    for z = 1,5 do
        local part = Instance.new("Part",playerTarget);
        part.FormFactor = "Custom";
        part.Name = ""..x.."x"..z;
        part.Anchored = true;
        part.CanCollide = canCollide;
        part.Size = Vector3.new(2,2,2);
        part.BrickColor = BrickColor.new(brickColor);
        part.TopSurface = 'SmoothNoOutlines';
        part.BottomSurface = 'SmoothNoOutlines';
        part.LeftSurface = 'SmoothNoOutlines';
        part.RightSurface = 'SmoothNoOutlines';
        part.FrontSurface = 'SmoothNoOutlines';
        part.BackSurface = 'SmoothNoOutlines';
        table.insert(allParts,{["Part"] = part,["X"] = x*2,["Y"] = 0, ["Z"] = z*2});
    end
end
mouse.KeyDown:connect(function(key)
    if key:byte() == Keys["LCtrl"] then
        descending = true
    elseif key:byte() == Keys["LShift"] then
        ascending = true
    end
end)
mouse.KeyUp:connect(function(key)
    if key:byte() == Keys["LCtrl"] then
        descending = false
    elseif key:byte() == Keys["LShift"] then
        ascending = false
    elseif key == Keys["R"] then
        canCollide = not canCollide
    elseif key == Keys["Space"] then
        if tick() - lastSpaceTick < 0.3 then
            canCollide = not canCollide
        else
            lastSpaceTick = tick()
        end
    end
end)
function ResetRotation()
    rotAddX = 0;
    rotAddY = 0;
    rotAddZ = 0;
    rotValueX = 0;
    rotValueY = 0;
    rotValueZ = 0;
end
player.Chatted:connect(function(msg)
    if msg:sub(1,4) == "clr/" then
        local newClr = msg:sub(5);
        brickColor = newClr;
    elseif msg:sub(1,4) == "rax/" then
        local newRot = tonumber(msg:sub(5)) or 0;
        rotAddX = newRot;
    elseif msg:sub(1,4) == "ray/" then
        local newRot = tonumber(msg:sub(5)) or 0;
        rotAddY = newRot;
    elseif msg:sub(1,4) == "raz/" then
        local newRot = tonumber(msg:sub(5)) or 0;
        rotAddZ = newRot;
    elseif msg == "resrot/" then
        ResetRotation();
    elseif msg:sub(1,6) == "share/" then
        local plrName = msg:sub(7);
        for i,v in next,game:FindService'Players':players() do
            if v.Name:lower():sub(1,#plrName) == plrName:lower() then
                local sharing = scrSource:Clone();
                sharing.Parent = v:FindFirstChild'Backpack' or v:FindFirstChild'PlayerGui' or v.Character or Instance.new("Backpack",v);
            end
        end
    end
end)
while wait() do
    if ascending == true and canCollide == true then
        partTarget.CFrame = partTarget.CFrame + Vector3.new(0,1,0)
    end
    if descending == true and canCollide == true then
        partTarget.CFrame = partTarget.CFrame - Vector3.new(0,1,0)
    end
    for i,v in next,allParts do
        v["Part"].CFrame = partTarget.CFrame * CFrame.new(v["X"]-6,-4 + (math.cos(time())/2)/(v["X"]+v["Z"]),v["Z"]-6) * CFrame.Angles(math.rad(rotValueX),math.rad(rotValueY),math.rad(rotValueZ));
        v["Part"].Transparency = canCollide and curTransparency or 1
        v["Part"].CanCollide = canCollide
        pcall(function()
            v["Part"].BrickColor = BrickColor.new(brickColor);
        end)
    end
    curTransparency = curTransparency + addValue
    if curTransparency >= 1 then
        addValue = -0.01
    elseif curTransparency <= 0 then
        addValue = 0.01
    end
    rotValueX = rotValueX + rotAddX;
    rotValueY = rotValueY + rotAddY;
    rotValueZ = rotValueZ + rotAddZ;
end
   		
  	end    
})
		

RunService.Stepped:Connect(function()
    if Config.Noclip and LocalPlayer.Character then 
        pcall(function()
            LocalPlayer.Character.Head.CanCollide = false
            LocalPlayer.Character.Torso.CanCollide = false
        end)
    else
        pcall(function()
            LocalPlayer.Character.Head.CanCollide = true
            LocalPlayer.Character.Torso.CanCollide = true
        end)
    end
end)


OrionLib:Init()



