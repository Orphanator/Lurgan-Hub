local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Lurgan Hub", HidePremium = false, Introtext = "The fog is coming", SaveConfig = true, ConfigFolder = "LurganTest"})

-- Valules

-- Functions
function IY()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end

function CMDX()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
end

function DHUB()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/dazaims/DA_Hub/main/DA_Hub", true))()
end

function Criminality()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/JackHiggly/RobloxThings/main/FemWare0", true))()
end

function Counterblox()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/NooVster/404/main/Source'))()
end

function PLSDonate()
    loadstring(game:HttpGet('https://tubhub.cloud/scripts/prettyplease.lua', false))()
end

function DaHood()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/spacexrandom/Lua/main/DaHood"))()
end

function AO()
    getgenv().toggle = true
    getgenv().whitelist = {"Elite Armor Chest","Elite Sailor Chest","Great Sailor Chest", "Golden Chest","Silver Chest","Food Chest" }; -- these are the chest that will be grabbed:)
    getgenv().speed = 100  --the higher the number the faster you go,increasing may lead too bans!
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Disttt/-Arcane-Odyssey/main/Autochest.lua"))()
end

function PL()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Denverrz/scripts/master/PRISONWARE_v1.3.txt"))()
end

function PS()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Muhammad6196/Project-WD/main/Mainstring.lua"))()
end

function JB()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/dazaims/DA_Hub/main/DA_Hub", true))()
end

function PF()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/mopsfl/rbxmopshub/main/loader.lua", true))()
end

function BP()
loadstring(game:HttpGet("https://raw.githubusercontent.com/MostafaXc00dy/MostafaXc00dy/main/Outlaws%20Hub/Breaking%20Point%20V7.lua"))()
end

function ND()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/2dgeneralspam1/scripts-and-stuff/master/scripts/LoadstringUjHI6RQpz2o8", true))()
 
end

function TS()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/MirayXS/scripts-4/main/TurtleSpy.lua", true))()
end

function SS()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Orphanator/Script/main/SimpleSpy", true))()
end

function LB()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Orphanator/Script/main/LuckyBlocks", true))()
end

function SB()
	loadstring(game:HttpGet("https://catware.vercel.app", true))()
end

function MM2()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Doggo-cryto/EclipseMM2/master/Script", true))() 
end

-- Tabs
