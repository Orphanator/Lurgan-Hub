function WalkingSpeed()
local WS = 16
game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = WS
end

function NoclipNotify()
    OrionLib:MakeNotification({
        Name = "Noclip Toggled",
        Content = "Keybind E was pressed",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end


