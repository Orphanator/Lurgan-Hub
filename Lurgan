Humanoid:AddSlider({
	Name = "Speed",
	Min = 0,
	Max = 1000,
	Default = 16,
	Color = Color3.fromRGB(255,0,0),
	Increment = 1,
	ValueName = "Walkspeed",
	Callback = function(e)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = e
	end
	})

	Humanoid:AddSlider({
		Name = "Jump",
		Min = 0,
		Max = 1000,
		Default = 50,
		Color = Color3.fromRGB(255,0,0),
		Increment = 1,
		ValueName = "JumpHeight",
		Callback = function(e)
			game.Players.LocalPlayer.Character.Humanoid.JumpPower = e
		end
		})

	Humanoid:AddSlider({
		Name = "Gravity",
		Min = 0,
		Max = 1000,
		Default = 150,
		Color = Color3.fromRGB(255,0,0),
		Increment = 1,
		ValueName = "Gravity",
		Callback = function(e)
			workspace.Gravity = e
		end
		})


		local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

getgenv().Config = Config or {
    Noclip = false
}

Humanoid:AddBind({
    Name = "Noclip",
    Default = Enum.KeyCode.E,
    Hold = false,
    Callback = function(state)
        Config.Noclip = not Config.Noclip 
        print(Config.Noclip)
        NoclipNotify()
    end
})

local old 
old = hookmetamethod(game, "__newindex", newcclosure(function(self, Meme, Value)
    if self == "HumanoidRootPart" or self == "Torso" and not checkcaller() then 
        if Meme == "CFrame" and self:IsDescendantOf(LocalPlayer.Character) then 
            return true
        end
    end

    return old(self, Meme, Value)
end))

RunService.Stepped:Connect(function()
    if Config.Noclip and LocalPlayer.Character then 
        pcall(function()
            LocalPlayer.Character.Head.CanCollide = false
            LocalPlayer.Character.Torso.CanCollide = false
        end)
    else
        pcall(function()
            LocalPlayer.Character.Head.CanCollide = true
            LocalPlayer.Character.Torso.CanCollide = true
        end)
    end
end)
		
Humanoid:AddBind({
    Name = "Platform Fly",
    Default = Enum.KeyCode.x,
    Hold = false,
    Callback = function(Value)
        print()
    end
})

	



OrionLib:Init()
